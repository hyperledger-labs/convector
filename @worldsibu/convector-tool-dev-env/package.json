  "peerDependencies": {
    "fabric-ca-client": ">=1.3.0",
    "fabric-client": ">=1.3.0"
  },
  "dependencies": {
    "@worldsibu/convector-common-fabric-helper": "1.2.0",
    "@theledger/fabric-chaincode-utils": "^4.0.0",
    "commander": "^2.16.0",
    "fabric-ca-client": "^1.3.0",
    "fabric-client": "^1.3.0",
    "shelljs": "^0.8.2",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "2.8.3"
  }
