{
  "name": "@worldsibu/convector-tool-dev-env",
  "version": "1.2.0",
  "description": "Development environment",
  "main": "./dist/index.js",
  "license": "Apache-2.0",
  "typings": "./dist/index.d.ts",
  "files": [
    "bin/*",
    "dist/*",
    "config/*",
    "scripts/*",
    "examples/*",
    "network-objects/*",
    "docker-compose.yml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "dev-env-start": "./scripts/start.sh",
    "dev-env-stop": "./scripts/stop.sh",
